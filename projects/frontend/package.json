{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "update-api-schema": "wget -q http://127.0.0.1:8000/api/openapi.json -O openapi.json && prettier --write ./openapi.json",
    "create-api-types": "orval --config orval.config.ts",
    "api": "npm run update-api-schema && npm run create-api-types",
    "prettify": "prettier src --write",
    "prettier": "prettier src -c"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.76.1",
    "axios": "^1.9.0",
    "next": "^15.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "^15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "orval": "^7.9.0",
    "postcss": "^8.4.49",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
